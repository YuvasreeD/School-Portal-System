<%- include("header") -%>
<div class="report">
    <h1>Virtual Report Card</h1>
    <p>Name: <%= nm %> </p>
    <p>Register Number: <%= rn %> </p>
    <p>Class: <%= cs %> </p>
    <p>Exam: <%= ex %> </p><br>

    <table class="table table-striped">
        <thead>
          <tr>
            <th scope="col">Subjects</th>
            <th scope="col">Marks</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>Maths</td>
            <td><%= ma %></td>
            
          </tr>
          <tr>
            <td>Science</td>
            <td><%= sci %></td>
          </tr>
          <tr>
            <td>Social</td>
            <td><%= soc %></td>
          </tr>
          <tr>
            <td>English</td>
            <td><%= eng %></td>
          </tr>
          <tr>
            <td>Language</td>
            <td><%= lang %></td>
          </tr>
        </tbody>
      </table>
      <br><br>

      <table class="table table-striped table-dark">
        <thead>
          <tr>
            <th scope="col">Total</th>
            <th scope="col">Percentage</th>
            <th scope="col">Rank</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td><%= tt %></td>
            <td><%= pr %></td>
            <td><%= rnk %></td>
          </tr>
        </tbody>
      </table>

</div>
<%- include("footer") -%>
